{
  "name": "Are you alive?",
  "short_name": "AYA",
  "version": "1.0.1",
  "version_name": "v1.0.1",
  "author": "B3none",
  "manifest_version": 3,
  "description": "Automatically clicks yes to the \"Are you still watching?\" popups on YouTube.",
  "background": {
    "service_worker": "js/i-am-alive.js"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ]
}
